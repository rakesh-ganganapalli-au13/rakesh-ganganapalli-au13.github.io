<%- include('../../layouts/adminHeader.ejs') %>

<h2 class="page-title">Add a new page</h2>
<a href="/api/admin/pages" class="btn btn-primary">Back to All Pages</a>



<form method="POST" action="/api/admin/pages/add-page/">

   
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" name = "title" value="<%= title %>" placeholder="Enter title" class="form-control">
    </div>

    <div class="form-group">
        <label for="slug">Slug</label>
        <input type="text" name = "slug" value="<%= slug%>" placeholder="Enter slug" class="form-control">
    </div>

    
    <div class="form-group">
        <label for="contant">Contant</label>
        <textarea name="contant" cols="20" rows="10" value="<%=contant%>" placeholder="Enter contant" class="form-control"></textarea>
        
    </div>
    
    <button class="btn btn-primary">Submit</button> 
   

</form>

<%- include('../../layouts/adminFooter.ejs') %>